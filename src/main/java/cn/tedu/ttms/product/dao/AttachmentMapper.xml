<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.ttms.product.dao.AttachmentDao">
   
    <select id="findObjects"
            resultType="attachment">
       select * 
       from tms_attachements
       order by createdTime desc
    </select>

    <select id="findObjectByDigest"
            resultType="attachment">
            select *
            from tms_attachements
            where fileDisgest=#{digest}
    </select>
    <insert id="insertObject">
            insert into tms_attachements
            (title,fileName,contentType,
            filePath,fileDisgest,
            createdTime,modifiedTime)
            values(
            #{title},#{fileName},#{contentType},
            #{filePath},#{fileDisgest},now(),now()
            )
    </insert>
    
    
    
    
   
</mapper>

